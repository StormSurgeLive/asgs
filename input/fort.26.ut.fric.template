$*************************HEADING************************
$
PROJ 'ASGS' '2011'
'StormName: %StormName%'
'EnsembleID: %EnsembleID%'
'WTIMINC Line in ADCIRC fort.15: %WTIMINC%'
$
$********************MODEL INPUT*************************
$
SET LEVEL 0.0
SET DEPMIN 0.1
MODE NONSTATIONARY
$
COORDINATES SPHERICAL CCM
$
CGRID UNSTRUCTURED CIRCLE MDC=36 FLOW=0.031384 MSC=40
READ UNSTRUCTURED
$
INIT ZERO
$
%hotstart%
$
INPGRID WLEV UNSTRUCTURED EXCEPTION 0.1  NONSTAT %startdatetime% %swandt% SEC %enddatetime%
READINP ADCWL
$
INPGRID  CUR UNSTRUCTURED EXCEPTION 0.   NONSTAT %startdatetime% %swandt% SEC %enddatetime%
READINP ADCCUR
$
INPGRID WIND UNSTRUCTURED EXCEPTION 0.   NONSTAT %startdatetime% %swandt% SEC %enddatetime%
READINP ADCWIND
$
INPGRID FRIC UNSTRUCTURED EXCEPTION 0.05 NONSTAT %startdatetime% %swandt% SEC %enddatetime%
READINP ADCFRIC
$
GEN3 KOMEN AGROW
WCAP KOMEN 2.36E-5 3.02E-3 2.0 1.0 1.0
BREAKING
FRICTION MADSEN KN=0.05
PROP BSBT
NUM STOPC DABS=0.005 DREL=0.01 CURVAT=0.005 NPNTS=95 NONSTAT MXITNS=20 CSIGMA CFL=0.25 CTHETA CFL=0.25
$
$*************************************************************
$
QUANTITY HS DIR TMM10 TMM10 FMIN=0.031384 FMAX=1.420416
$
TEST 1,0
COMPUTE %startdatetime% %swandt% SEC %enddatetime%
STOP
$
